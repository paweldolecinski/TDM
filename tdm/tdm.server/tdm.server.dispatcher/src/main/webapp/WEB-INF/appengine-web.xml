<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>supdicium</application>
	<version>2</version>
	<threadsafe>true</threadsafe>

	<sessions-enabled>true</sessions-enabled>
	<static-files>
		<include path="SupDicium.html" expiration="30d" />
		<include path="SupDicium/**.cache.*" expiration="30d" />
		<include path="SupDicium/js/*.js" expiration="30d" />
	</static-files>
	<resource-files>
		<include path="SupDicium/*.gwt.rpc" />
		<include path="SupDicium/*.nocache.*" />
	</resource-files>
	
	<precompilation-enabled>true</precompilation-enabled>

	<!-- Configure java.util.logging -->
	<system-properties>
		<property name="java.util.logging.config.file" value="WEB-INF/properties/logging.properties" />
	</system-properties>

</appengine-web-app>