<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with type="com.tdm.client.resources.AppResources"
		field="res" />

	<g:HTMLPanel styleName="{res.getMainCss.problemListItem} {res.getMainCss.thumbnail}">
		<g:InlineHyperlink ui:field="titleImage" />
		<g:HTMLPanel styleName="{res.getMainCss.caption}">
			<h3 class="{res.getMainCss.title}">
				<g:InlineHyperlink ui:field="title" />
			</h3>
			<p class="{res.getMainCss.meta}">
				by Paweł Doleciński on 26 November 2012
			</p>
			<p ui:field="description" class="{res.getMainCss.description}" />
		</g:HTMLPanel>
	</g:HTMLPanel>

</ui:UiBinder> 