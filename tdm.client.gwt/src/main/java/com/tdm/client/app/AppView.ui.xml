<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:tdm="urn:import:com.tdm.client.app.ui">

	<ui:with field="res" type="com.tdm.client.resources.AppResources" />

	<g:FlowPanel styleName="{res.content.rowFluid}">
		<g:FlowPanel styleName="{res.content.span8} {res.content.indexMain}">

			<g:FlowPanel styleName="{res.content.indexLeft}">
			</g:FlowPanel>
			<g:FlowPanel styleName="{res.content.indexCenter}"
				ui:field="mainContentPanel" />

		</g:FlowPanel>

		<g:FlowPanel styleName="{res.content.span4} {res.content.indexRight}" ui:field="rightContentPanel">
		</g:FlowPanel>

	</g:FlowPanel>

</ui:UiBinder>