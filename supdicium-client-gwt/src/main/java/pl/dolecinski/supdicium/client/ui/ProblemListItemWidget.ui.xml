<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with type="pl.dolecinski.supdicium.client.theme.base.MainCss"
		field="res" />

	<g:HTMLPanel styleName="{res.problemListItem} {res.thumbnail}">
		<g:InlineHyperlink ui:field="titleImage" />
		<g:HTMLPanel styleName="{res.caption}">
			<h3 class="{res.title}">
				<g:InlineHyperlink ui:field="title" />
			</h3>
			<p class="{res.meta}">
				by Paweł Doleciński on 26 November 2012
			</p>
			<p ui:field="description" class="{res.description}" />
		</g:HTMLPanel>
	</g:HTMLPanel>

</ui:UiBinder> 